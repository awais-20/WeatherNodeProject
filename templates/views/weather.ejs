<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./partials/head') %>
</head>
<%- include('./partials/navbar') %>

<body>
  <div class="container mt-5">
    <div class="row">
      <div class="col-md-10 col-12 mx-auto">
        <div class="main-content">
          <form class="temp_form">
            <input type="text" class="form-control" id="searchName" placeholder="Enter city name" aria-label="Enter city name">
            <input type="submit" class="btn btn-primary" value="search" id="submitbtn">
          </form>
        </div>
      </div>

      <div class="tempInformation bg-dark text-white mt-2">
        <div class="top-layer">
          <p id="day">Tuesday</p>
          <p id="Today_date">10 AUGUST</p>
        </div>
        <div class="main-layer">
          <p id="city_name">Get Output Here</p>
      
        <div class="middle_layer data_hide">
          <p id="temp"><span id="temp_real_val">0</span><sup>o</sup>C</p>
          <p id="temp_status"><i class="fa fa-cloud" aria-hidden="true"></i></p>
        </div>
      </div>
      </div>
    </div>
  </div>

  <%- include('./partials/footer') %>
  <script src="js/main.js"></script>
</body>

</html>
